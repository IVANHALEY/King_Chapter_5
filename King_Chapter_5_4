#include <stdio.h>

int main() {
	float speed = 0; 

	printf("Enter speed in knots: "); 
	scanf_s("%f", &speed); 
	if (speed < 1) {
		printf("Calm"); 
		}
	else if (speed < 4) {
		printf("Light air"); 
	}
	else if (speed < 28) {
		printf("Breeze"); 
	}
	else if (speed < 48) {
		printf("Gale"); 
	}
	else if (speed < 64) {
		printf("Storm"); 
	}
	else if (speed > 63) {
		printf("Hurricane"); 
	}
}
